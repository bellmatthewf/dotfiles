{
    "tabout.disableByDefault": true,
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.startupEditor": "none",
    "workbench.colorTheme": "Monokai",
    "workbench.iconTheme": "Monokai Classic Icons",
    "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
    "javascript.format.insertSpaceAfterConstructor": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    // Apply formatter extension (vetur formatter, editor config)
    // Equivalent to pressing
    // Shift + Option + F before saving
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.detectIndentation": true,
    "editor.formatOnSave": true,
    "editor.stickyScroll.enabled": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.gotoLocation.multipleDefinitions": "goto",
    // VSCode ESLint - autoapply fix lint on save
    "editor.codeActionsOnSave": {
        "source.organizeImports": "always"
    },
    // Code Runner Extension
    "code-runner.executorMap": {
        "python": "python $fullFileName"
    },
    "code-runner.enableAppInsights": false,
    "code-runner.showExecutionMessage": false,
    "code-runner.clearPreviousOutput": true,
    "code-runner.saveAllFilesBeforeRun": true,
    "code-runner.runInTerminal": true,
    // Git
    "git.autofetch": true,
    // VSCode Python
    "python.languageServer": "Pylance",
    "python.analysis.typeCheckingMode": "basic",
    "python.terminal.activateEnvironment": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "vetur.format.options.useTabs": false,
    "vetur.format.options.tabSize": 4,
    "vetur.ignoreProjectWarning": true,
    // Import Cost Extension
    "importCost.timeout": 1000000,
    "prettier.tabWidth": 4,
    "prettier.useTabs": false,
    "[markdown]": {
        "editor.wordWrap": "wordWrapColumn",
        "editor.wordWrapColumn": 80
    },
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.codeActionsOnSave": {
            "source.organizeImports": "never"
        }
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[makefile]": {
        "editor.insertSpaces": false,
        "editor.detectIndentation": false
    },
    "redhat.telemetry.enabled": true,
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "editor.minimap.enabled": false,
    "[python]": {
        "editor.formatOnType": true,

        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1"
    },
    "editor.accessibilitySupport": "off",
    "workbench.editor.empty.hint": "hidden",
    "files.associations": {
        "*.erb": "erb",
        "*.vue": "vue"
    },
    "window.zoomLevel": 1,
    "testing.followRunningTest": false,
    "python.testing.autoTestDiscoverOnSaveEnabled": false
    // "black-formatter.path": [".venv/bin/black"]
}
